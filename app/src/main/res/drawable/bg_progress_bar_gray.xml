<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@android:id/background">
    <shape>
      <corners android:radius="@dimen/_200dp" />
      <gradient
          android:startColor="@color/gray_dark_3"
          android:centerColor="@color/gray_dark_3"
          android:centerY="0.75"
          android:endColor="@color/gray_dark_3"
          android:angle="270"
          />
    </shape>
  </item>

  <item android:id="@android:id/secondaryProgress">
    <clip>
      <shape>
        <corners android:radius="@dimen/_200dp" />
        <gradient
            android:startColor="@color/gray_dark_3"
            android:centerColor="@color/gray_dark_3"
            android:centerY="0.75"
            android:endColor="@color/gray_dark_3"
            android:angle="270"
            />
      </shape>
    </clip>
  </item>
  <item
      android:id="@android:id/progress"
      >
    <clip>
      <shape>
        <corners
            android:radius="@dimen/_200dp" />
        <gradient
            android:startColor="@color/gold"
            android:endColor="@color/gold"
            android:angle="270" />
      </shape>
    </clip>
  </item>
</layer-list>