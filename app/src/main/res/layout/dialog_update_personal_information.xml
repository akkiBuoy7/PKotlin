<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="handlers"
            type="plm.patientslikeme2.ui.main.view.dialog.profile.UpdatePersonalInformationDialog" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/_4dp">

        <androidx.core.widget.NestedScrollView
            android:id="@+id/parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg_bottom_sheet_parent"
                android:backgroundTint="@color/white"
                android:padding="@dimen/_16dp">

                <include
                    android:id="@+id/ll_error_success"
                    layout="@layout/layout_success_error"
                    android:visibility="gone"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <RelativeLayout
                    android:id="@+id/rl_header"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/ll_error_success">

                    <plm.patientslikeme2.utils.usercontrol.UserTextView
                        android:id="@+id/tv_header"
                        style="@style/TextViewBold16"
                        android:layout_centerInParent="true"
                        android:gravity="center"
                        android:text="@string/edit_personal_information" />

                    <ImageView
                        android:id="@+id/iv_close"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_centerInParent="true"
                        android:layout_gravity="end"
                        android:contentDescription="@null"
                        android:onClick="@{handlers::onItemClicked}"
                        android:src="@drawable/ic_close_dark_green"
                        app:tint="@color/gray_dark_4" />
                </RelativeLayout>

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_photo"
                    style="@style/TextView20"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_16dp"
                    android:text="@string/photo"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rl_header" />

                <LinearLayout
                    android:id="@+id/ll_photo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_photo">

                    <plm.patientslikeme2.utils.usercontrol.HexagonImageView
                        android:id="@+id/iv_user"
                        android:layout_width="@dimen/_90dp"
                        android:layout_height="@dimen/_90dp"
                        android:layout_gravity="center"
                        app:siShape="@drawable/ic_hexagon_mask"
                        android:src="@color/white" />

                    <plm.patientslikeme2.utils.usercontrol.UserButton
                        android:id="@+id/btn_change_photo"
                        style="@style/ButtonDarkGreenBorder"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_30dp"
                        android:layout_marginHorizontal="@dimen/_8dp"
                        android:onClick="@{handlers::onItemClicked}"
                        android:text="@string/change_photo"
                        android:textSize="@dimen/_13sp" />
                </LinearLayout>

                <View
                    android:id="@+id/divider_line_1"
                    style="@style/DotLineView"
                    android:layout_marginVertical="@dimen/_16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_photo" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_profile_headline"
                    style="@style/TextView20"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_16dp"
                    android:text="@string/profile_headline"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider_line_1" />

                <plm.patientslikeme2.utils.usercontrol.UserEditText
                    android:id="@+id/et_profile_headline"
                    style="@style/EditText"
                    android:layout_height="@dimen/_150dp"
                    android:layout_marginVertical="@dimen/_8dp"
                    android:gravity="center|top|start"
                    android:hint="@string/profile_headline"
                    android:inputType="textMultiLine"
                    android:maxLength="160"
                    android:padding="@dimen/_8dp"
                    android:textSize="@dimen/_16sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_profile_headline" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_profile_headline_count"
                    style="@style/TextView13"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/n_160_characters"
                    android:textColor="@color/gray_dark_5"
                    android:translationY="-0.25sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_profile_headline" />

                <View
                    android:id="@+id/divider_line_2"
                    style="@style/DotLineView"
                    android:layout_marginVertical="@dimen/_16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_profile_headline_count" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_demographics"
                    style="@style/TextView20"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_16dp"
                    android:text="@string/demographics"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider_line_2" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_birthdate"
                    style="@style/TextViewBold16"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_16dp"
                    android:text="@string/birthdate"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_demographics" />

                <LinearLayout
                    android:id="@+id/ll_birthdate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8dp"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_birthdate">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="@dimen/_10dp"
                            android:text="@string/month" />

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            android:id="@+id/tv_month"
                            style="@style/Spinner"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:onClick="@{handlers::onItemClicked}"
                            android:paddingHorizontal="@dimen/_16dp"
                            android:tag="9" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_10dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_marginHorizontal="@dimen/_10dp"
                            android:text="@string/day" />

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            android:id="@+id/tv_day"
                            style="@style/Spinner"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:onClick="@{handlers::onItemClicked}"
                            android:paddingHorizontal="@dimen/_16dp"
                            android:tag="9" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_marginHorizontal="@dimen/_10dp"
                            android:text="@string/year" />

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            android:id="@+id/tv_year"
                            style="@style/Spinner"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:onClick="@{handlers::onItemClicked}"
                            android:paddingHorizontal="@dimen/_16dp"
                            android:tag="9" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_gender"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10dp"
                    android:baselineAligned="false"
                    android:orientation="horizontal"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_birthdate">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="@dimen/_10dp"
                            android:text="@string/sex" />

                        <androidx.appcompat.widget.AppCompatSpinner
                            android:id="@+id/spinner_sex"
                            style="@style/Spinner"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:paddingHorizontal="@dimen/_8dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_marginHorizontal="@dimen/_10dp"
                            android:text="@string/gender_identity" />

                        <androidx.appcompat.widget.AppCompatSpinner
                            android:id="@+id/spinner_gender_identity"
                            style="@style/Spinner"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:paddingHorizontal="@dimen/_8dp" />
                    </LinearLayout>
                </LinearLayout>

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_ethnicity"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/ethnicity"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_gender" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_ethnicity"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"

                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_ethnicity" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_highest_level_of_education"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/highest_level_of_education"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_ethnicity" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_highest_level_of_education"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_highest_level_of_education" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_insurance"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/insurance"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_highest_level_of_education" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_insurance"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_insurance" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_military_status"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/military_status"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_insurance" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_military_status"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_military_status" />

                <LinearLayout
                    android:id="@+id/ll_start_of_service"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10dp"
                    android:orientation="vertical"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_military_status">

                    <plm.patientslikeme2.utils.usercontrol.UserTextView
                        style="@style/TextViewBold16"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_8dp"
                        android:text="@string/start_of_service"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/spinner_insurance" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8dp"
                        android:baselineAligned="false"
                        android:orientation="horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ll_birthdate">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <plm.patientslikeme2.utils.usercontrol.UserTextView
                                style="@style/TextView16"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="@dimen/_10dp"
                                android:text="@string/year"
                                app:textAllCaps="true" />

                            <androidx.appcompat.widget.AppCompatSpinner
                                android:id="@+id/spinner_start_year"
                                style="@style/Spinner"
                                android:layout_gravity="center"
                                android:gravity="center|start"
                                android:paddingHorizontal="@dimen/_8dp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10dp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="@dimen/_10dp"
                                android:orientation="horizontal">

                                <plm.patientslikeme2.utils.usercontrol.UserTextView
                                    style="@style/TextView16"
                                    android:layout_width="wrap_content"
                                    android:text="@string/month"
                                    app:textAllCaps="true" />

                                <plm.patientslikeme2.utils.usercontrol.UserTextView
                                    style="@style/TextView13"
                                    android:layout_width="wrap_content"
                                    android:maxLines="1"
                                    android:paddingStart="@dimen/_2dp"
                                    android:text="@string/if_you_know" />
                            </LinearLayout>

                            <androidx.appcompat.widget.AppCompatSpinner
                                android:id="@+id/spinner_start_month"
                                style="@style/Spinner"
                                android:layout_gravity="center"
                                android:gravity="center|start"
                                android:paddingHorizontal="@dimen/_8dp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_end_of_service"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10dp"
                    android:orientation="vertical"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_start_of_service">

                    <plm.patientslikeme2.utils.usercontrol.UserTextView
                        style="@style/TextViewBold16"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_8dp"
                        android:text="@string/end_of_service"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/spinner_insurance" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8dp"
                        android:baselineAligned="false"
                        android:orientation="horizontal"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ll_birthdate">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <plm.patientslikeme2.utils.usercontrol.UserTextView
                                style="@style/TextView16"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="@dimen/_10dp"
                                android:text="@string/year"
                                app:textAllCaps="true" />

                            <androidx.appcompat.widget.AppCompatSpinner
                                android:id="@+id/spinner_end_year"
                                style="@style/Spinner"
                                android:layout_gravity="center"
                                android:gravity="center|start"
                                android:paddingHorizontal="@dimen/_8dp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_10dp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="@dimen/_10dp"
                                android:orientation="horizontal">

                                <plm.patientslikeme2.utils.usercontrol.UserTextView
                                    style="@style/TextView16"
                                    android:layout_width="wrap_content"
                                    android:text="@string/month"
                                    app:textAllCaps="true" />

                                <plm.patientslikeme2.utils.usercontrol.UserTextView
                                    style="@style/TextView13"
                                    android:layout_width="wrap_content"
                                    android:maxLines="1"
                                    android:paddingStart="@dimen/_2dp"
                                    android:text="@string/if_you_know" />
                            </LinearLayout>

                            <androidx.appcompat.widget.AppCompatSpinner
                                android:id="@+id/spinner_end_month"
                                style="@style/Spinner"
                                android:layout_gravity="center"
                                android:gravity="center|start"
                                android:paddingHorizontal="@dimen/_8dp" />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_military_branch"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/military_branch"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_end_of_service" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_military_branch"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_military_branch" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_military_rank"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/military_rank"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_military_branch" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_military_rank"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_military_rank" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_military_job"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/military_job"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_military_rank" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_military_job"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_military_job" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_military_combat_zone"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/did_you_ever_deploy_to_a_formally_designated_combat_zone"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_military_job" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_military_combat_zone"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_military_combat_zone" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_va_health_care_system"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/are_you_eligible_for_or_enrolled_in_the_va_healthcare_system"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_military_combat_zone" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_military_eligibility"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_va_health_care_system" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_location"
                    style="@style/TextViewBold16"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_16dp"
                    android:text="@string/location"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_military_eligibility" />

                <LinearLayout
                    android:id="@+id/ll_location"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10dp"
                    android:baselineAligned="false"
                    android:gravity="center"
                    android:orientation="horizontal"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_location">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="@dimen/_8dp"
                            android:text="@string/country" />

                        <plm.patientslikeme2.utils.usercontrol.UserAutoCompleteTextView
                            android:id="@+id/et_country"
                            style="@style/EditText"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:hint="@string/hint"
                            android:inputType="text"
                            android:paddingHorizontal="@dimen/_16dp"
                            android:tag="9"
                            android:textSize="@dimen/_16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="@dimen/_10dp"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_marginHorizontal="@dimen/_10dp"
                            android:text="@string/zip_code" />

                        <plm.patientslikeme2.utils.usercontrol.UserEditText
                            android:id="@+id/et_zip_code"
                            style="@style/EditText"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:hint="@string/hint"
                            android:inputType="text"
                            android:paddingHorizontal="@dimen/_16dp"
                            android:tag="9"
                            android:textSize="@dimen/_16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            style="@style/TextView16"
                            android:layout_marginHorizontal="@dimen/_10dp" />

                        <plm.patientslikeme2.utils.usercontrol.UserTextView
                            android:id="@+id/tv_city"
                            style="@style/TextView16"
                            android:layout_marginHorizontal="@dimen/_10dp"
                            android:gravity="center"
                            android:text="@string/empty_string" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    android:id="@+id/divider_line_3"
                    style="@style/DotLineView"
                    android:layout_marginVertical="@dimen/_16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ll_location" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_my_story"
                    style="@style/TextView20"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_16dp"
                    android:text="@string/my_story"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider_line_3" />

                <plm.patientslikeme2.utils.usercontrol.UserEditText
                    android:id="@+id/et_my_story"
                    style="@style/EditText"
                    android:layout_height="@dimen/_200dp"
                    android:layout_marginVertical="@dimen/_8dp"
                    android:gravity="center|top|start"
                    android:hint="@string/my_story"
                    android:inputType="textMultiLine"
                    android:padding="@dimen/_8dp"
                    android:textSize="@dimen/_16sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_my_story" />

                <View
                    android:id="@+id/divider_line_4"
                    style="@style/DotLineView"
                    android:layout_marginVertical="@dimen/_16dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_my_story" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_interests"
                    style="@style/TextView20"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_16dp"
                    android:text="@string/interests"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/divider_line_4" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_primary_interest"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/primary_interest"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_interests" />

                <androidx.appcompat.widget.AppCompatSpinner
                    android:id="@+id/spinner_primary_interest"
                    style="@style/Spinner"
                    android:layout_gravity="center"
                    android:gravity="center|start"
                    android:paddingHorizontal="@dimen/_8dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_primary_interest" />

                <plm.patientslikeme2.utils.usercontrol.UserTextView
                    android:id="@+id/tv_secondary_interests"
                    style="@style/TextView16"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_8dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:text="@string/secondary_interests"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/spinner_primary_interest" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rv_secondary_interests"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10dp"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/tv_secondary_interests"
                    tools:listitem="@layout/row_profile_interest" />

                <plm.patientslikeme2.utils.usercontrol.UserButton
                    android:id="@+id/btn_save"
                    style="@style/ButtonDarkGreen"
                    android:layout_width="wrap_content"
                    android:layout_margin="@dimen/_20dp"
                    android:layout_marginEnd="@dimen/_20dp"
                    android:onClick="@{handlers::onItemClicked}"
                    android:text="@string/save"
                    android:textSize="@dimen/_16sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/rv_secondary_interests" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>